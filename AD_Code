            position:function(dir,size){
                var $t=this,s=size,l,r,t,b,l1,r1,t1,b1;
                switch(dir){
                    case"left down":
                        l=2;
                        t='';
                        r='';
                        b=2;
                        break;
                    case"left up":
                        l=2;
                        t=2;
                        r='';
                        b='';
                        break;
                    case"right down":
                        l='';
                        t='';
                        r=2;
                        b=2;
                        break;
                    case"right up":
                        l='';
                        t=2;
                        r=2;
                        b='';
                        break;
                    case"center":
                        r='';
                        b='';
                        l=($t.screen().w-s.w)*0.5;
                        t=($t.screen().h-s.h)*0.5;
                        break;
                    default:
                        t=dir.split(" ")[0];
                        l=dir.split(" ")[1];
                        r='';
                        b='';
                        break
                };
                css($('.ad_'),"left:"+l+"px;top:"+t+"px;right:"+r+"px;bottom:"+b+"px;");
